{
  "name": "trade-backend",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"\n",
    "build": "tsc --noEmit",
    "start": "ts-node --transpile-only src/main.ts",
    "cli": "ts-node src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "ts-node src/lib/db/migrate.ts up",
    "migrate:status": "ts-node src/lib/db/migrate.ts status",
    "migrate:reset": "ts-node src/lib/db/migrate.ts reset",
    "swagger:generate": "ts-node src/swagger.ts"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.4.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.5",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.0",
    "@trade/filter": "^0.1.0",
    "better-sqlite3": "^9.6.0",
    "bullmq": "^5.25.4",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.19.2",
    "nestjs-zod": "^5.0.1",
    "openai": "^6.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "tinkoff-invest-api": "^7.0.1",
    "typeorm": "^0.3.20",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  }
}
