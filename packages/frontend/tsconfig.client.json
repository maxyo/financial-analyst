{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "lib": ["ES2020", "DOM"],
    "strict": false,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "jsx": "react",
    "rootDir": "ts",
    "outDir": "build"
  },
  "include": ["ts/**/*"]
}
